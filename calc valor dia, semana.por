programa {
  funcao inicio() {
    inteiro numhoras, valdia, valsemana
    escreva("Digite o número de horas trabalhadas no dia: ")
    leia(numhoras)
    valdia= numhoras * 30
    valsemana= valdia * 5
    escreva ("Valor recebido no dia é: ", valdia)
    escreva ("\nValor recebido na semana é: ", valsemana)

  }
}
